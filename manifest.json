{
  "manifest_version": 2,

  "name": "Facewash",
  "description": "This extension copy the current facebook link with only the relevant information.",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "*://www.facebook.com/*"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  }
}